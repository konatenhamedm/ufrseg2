<ul class="breadcrumb breadcrumb-separatorless fw-semibold fs-7 my-1">
    {% for item in items %}
        {% if not loop.last and item.route is defined %}
            <li class="breadcrumb-item">
                <a href="{{ path(item.route, item.args|default({})) }}">{{ item.label | default('Tableau de bord') }}</a>
            </li>
            <li class="breadcrumb-item">
				<span class="bullet bg-gray-200 w-5px h-2px"></span>
		    </li>
        {% else %}
            <li class="breadcrumb-item text-muted"><span>{{ item.label }}</span>
        {% endif %}
    {% else %}
        <li class="breadcrumb-item text-muted"><a href="{{ path('app_default') }}">Tableau de bord</a></li>
    {% endfor %}
</ul>

